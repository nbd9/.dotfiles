[core]
	excludesfile = ~/.dotfiles/.gitignore
	editor = code --wait
	pager = delta
[init]
	templatedir = ~/.dotfiles/GitHooks
[pull]
	rebase = true
[push]
	default = current
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
